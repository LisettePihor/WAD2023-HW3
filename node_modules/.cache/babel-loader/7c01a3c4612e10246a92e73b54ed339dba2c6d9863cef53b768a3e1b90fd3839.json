nLength &&\r\n          this.hasUpperCase &&\r\n          this.hasLowerCase &&\r\n          this.hasNumeric &&\r\n          this.startsWithUpperCase &&\r\n          this.hasUnderscore;\r\n    },\r\n    submitForm() {\r\n      // Your signup logic goes here\r\n      console.log('Signup form submitted');\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.signup {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.input {\r\n  margin: 5px;\r\n}\r\n\r\n.password-validation {\r\n  margin-top: 5px;\r\n}\r\n\r\n.no-bullets {\r\n  list-style-type: none;\r\n}\r\n\r\n.signup-button {\r\n\r\n}\r\n</style>"],"mappings":"AA8BA,eAAe;EACbA,QAAQ,EAAE;IACRC,YAAY,EAAE,SAAAA,CAAA,EAAW;MACvB,MAAMC,cAAa,GAAI,CAAC,IAAI,CAACC,QAAQ,CAACC,KAAK,CAAC,QAAQ,KAAK,EAAE,EAAEC,MAAM;MACnE,OAAOH,cAAa,IAAK,CAAC;IAC5B,CAAC;IACDI,YAAY,EAAE,SAAAA,CAAA,EAAW;MACvB,OAAO,IAAI,CAACH,QAAQ,CAACE,MAAK,IAAK,KAAK,IAAI,CAACF,QAAQ,CAACE,MAAK,GAAI,EAAE;IAC/D,CAAC;IACDE,UAAU,EAAE,SAAAA,CAAA,EAAY;MACtB,OAAO,IAAI,CAACC,IAAI,CAAC,IAAI,CAACL,QAAQ,CAAC;IACjC,CAAC;IACDM,aAAa,EAAE,SAAAA,CAAA,EAAY;MACzB,OAAO,GAAG,CAACD,IAAI,CAAC,IAAI,CAACL,QAAQ,CAAC;IAChC,CAAC;IACDO,YAAY,EAAE,SAAAA,CAAA,EAAY;MACxB,OAAO,OAAO,CAACF,IAAI,CAAC,IAAI,CAACL,QAAQ,CAAC;IACpC,CAAC;IACDQ,mBAAmB,EAAE,SAAAA,CAAA,EAAY;MAC/B,OAAO,QAAQ,CAACH,IAAI,CAAC,IAAI,CAACL,QAAQ,CAAC;IACrC;EACF,CAAC;EACDS,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZV,QAAQ,EAAE,EAAE;MACZW,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAAA,EAAG;MACjB;MACA,IAAI,CAACF,eAAc,GACf,IAAI,CAACR,YAAW,IAChB,IAAI,CAACI,YAAW,IAChB,IAAI,CAACT,YAAW,IAChB,IAAI,CAACM,UAAS,IACd,IAAI,CAACI,mBAAkB,IACvB,IAAI,CAACF,aAAa;IACxB,CAAC;IACDQ,UAAUA,CAAA,EAAG;MACX;MACAC,OAAO,CAACC,GAAG,CAAC,uBAAuB,CAAC;IACtC;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}